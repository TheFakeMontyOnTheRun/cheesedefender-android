<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent" android:background="#000044">
    <LinearLayout android:layout_width="fill_parent" android:id="@+id/linearLayout1" android:layout_height="wrap_content" android:orientation="vertical" android:layout_marginLeft="10dip" android:layout_marginRight="10dip" android:layout_marginTop="10dip" android:background="#00FF00">
        <TextView android:textAppearance="?android:attr/textAppearanceLarge" android:text="Level Finished!" android:layout_width="fill_parent" android:id="@+id/textView2" android:layout_height="wrap_content" android:textColor="#000000" android:layout_marginLeft="5dip" android:layout_marginRight="5dip" android:layout_marginTop="2dip"></TextView>
        <Button android:text="Ok!" android:layout_width="wrap_content" android:id="@+id/btnNextLevel" android:layout_marginTop="10dip" android:layout_height="wrap_content" android:layout_gravity="center"></Button>
    </LinearLayout>
    
</LinearLayout>
